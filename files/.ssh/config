Host bitbucket.org
   User git
   IdentityFile ~/.ssh/dpg_id_rsa
   HostName bitbucket.org

Host github.com
   User git
   IdentityFile ~/.ssh/id_rsa
   HostName github.com

Host i-* mi-* nj-*
  IdentityFile ~/.ssh/id_rsa    
  ProxyCommand ~/.local/bin/aws-ssm-ec2-proxy-command.sh %h %r %p ~/.ssh/id_rsa.pub
  StrictHostKeyChecking no
   
# njetflix  
Host nj-bastion  
  User ubuntu  
  hostname i-0f18004aed386d7f2  
   
Host nj-mon  
  User ubuntu  
  hostname i-014963b51e7de18c4  
   
Host nj-jenkins  
  User ubuntu  
  hostname i-0c2d3f3222e775f60  
   
Host nj-opensearch  
  User ubuntu  
  hostname i-0026345d91c2790ac

Host *
   User git
   IdentityFile ~/.ssh/id_rsa
   IdentitiesOnly yes
   UseKeychain yes
   #AddKeychain yes

Host mac_mini
  HostName 10.25.29.140
  User maestro
  IdentityFile ~/.ssh/id_rsa_mac_mini
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
